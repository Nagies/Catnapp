    <div class="create-container">

      <%= form_for Space.new do |f| %>
      <% if Space.new.errors.any? %>
      <div id="error_explanation">
        <h2><%= pluralize(@space.errors.count, "error") %> prohibited this space from being saved:</h2>

        <ul>
          <% Space.new.errors.full_messages.each do |message| %>
          <li><%= message %></li>
          <% end %>
        </ul>
      </div>
      <% end %>

      <div class="field">
        <%= f.label :address %><br>
        <%= f.text_field :address %>
      </div>
      <div class="field">
        <%= f.label :rate %><br>
        <%= f.number_field :rate %>
      </div>
      <div class="field">
        <%= f.label :description %><br>
        <%= f.text_area :description %>
      </div>
      <div class="field">
        <%= f.label :user_id %><br>
        <%= f.number_field :user_id %>
      </div>
      <div class="actions">
        <%= f.submit %>
      </div>
      <% end %>

    </div>


